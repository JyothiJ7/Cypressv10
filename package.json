    "cypress": "^10.9.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-drag-drop": "^1.1.1"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1"
  }
